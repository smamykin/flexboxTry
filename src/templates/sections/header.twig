{% from 'templates/macro/menu.twig' import menu %}
{% from 'templates/macro/logo.twig' import logo_header %}

{% set menuLinks = [
    'Home',
    'Service',
    'About us',
    'Pricing Table',
    'How it work',
    'Happy Clients',
    'Contact Us'
] %}
{% set items = [] %}
{% for item in menuLinks %}
    {% set items = items|merge([{title: item, link: '#'}]) %}
{% endfor %}
{{ menu(items) }}
{{ logo_header() }}

